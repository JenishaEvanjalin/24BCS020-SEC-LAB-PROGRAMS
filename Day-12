<body>
    <script>
        //set-> can't accepet duplicates , to uniquly identify the value's
        //create a set 
        let email=new Set([]);
        email.add("js@gmail.com");
        email.add("java@gmail.com");
        email.add("python@gmail.com");
        email.add("js@gmail.com");
        console.log(email);
        email.delete("js@gmail.com");
        console.log(email);


        for(let mail of email){
            console.log(mail);
        }

        email.forEach(x=>{
            console.log(x)
        })

        //example program
        let adminpermission=new Set(["Write","read","delete"]);
        let userpermission=new Set(["read"]);


        function hasPermission(role,permission){
            return role.has(permission);
        }
        console.log(hasPermission(adminpermission,"delete"));
        console.log(hasPermission(userpermission,"delete"));




        //Map:
        let user1={name:"jenisha"};
        let user2={name:"evanjalin"};
       // let map=new Map();
       let map=new WeakMap();
        map.set(user1,"welcome");
        map.set(user2,"Hai miruthula");
        console.log(map);
        map.delete(user2);
        console.log(map)


        for(let detail of map.values){
            console.log(detail);
        }
    </script>
